    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "vite": "^7.1.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
