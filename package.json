  "type": "module",
  "engines": {
    "node": ">=20.19.5",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "start": "node build/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/server.ts",
    "test": "test",
