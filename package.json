  },
  "homepage": "https://github.com/xakplant/project-sequentia#readme",
  "dependencies": {
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sequelize": "^6.37.7",
    "sirv": "^3.0.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
